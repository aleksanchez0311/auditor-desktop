	<queryString>
		<![CDATA[SELECT P.ID, O.ITEM AS PRODUCT, P.DESCRIPTION,SUM(O.AMMOUNT) AS AMMOUNT, SUM(AMMOUNT * P.HIGHEST_COST) AS VALUE , (SELECT ABREV FROM MEASURE_UNITS WHERE ID = P.MEASURE_UNIT) AS ABR
FROM ITEMS$P!{COPY} P
JOIN OPERATIONS$P!{COPY} O ON P.ID = O.ITEM
WHERE O.INCOME = FALSE
	AND O.OPENING = FALSE 
	 AND O.ENTITY = $P{ENTITY}
     AND SUBSTRING(P.id::text, LENGTH(SUBSTRING(p.entity::text FROM '^[0-9]+')) + 1, 1) IN ('1', '2') 
	AND O.DATE >= TO_TIMESTAMP($P{FDCP})
	AND O.DATE < TO_TIMESTAMP($P{FDNP})
GROUP BY P.ID,
	P.DESCRIPTION, O.ITEM
ORDER BY P.CODE]]>
	</queryString>
