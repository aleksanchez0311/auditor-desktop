	<queryString>
		<![CDATA[SELECT * FROM
	(SELECT P.DESCRIPTION, P.id, M.ABREV, 
			(SELECT SUM(CASE WHEN O.INCOME THEN O.AMMOUNT ELSE -O.AMMOUNT END) FROM OPERATIONS$P!{COPY} O WHERE O.ITEM = P.ID  AND O.ENTITY = $P{ENTITY} AND O.DATE < TO_TIMESTAMP($P!{FDCP})) AS AT_BEGIN,
			(SELECT SUM(O.AMMOUNT) FROM OPERATIONS$P!{COPY} O WHERE O.ITEM = P.ID AND O.ENTITY = $P{ENTITY} AND O.DATE >= TO_TIMESTAMP($P!{FDCP}) AND O.DATE < TO_TIMESTAMP($P!{FDNP}) AND O.INCOME = TRUE) AS INCOMES,
			(SELECT SUM(O.AMMOUNT) FROM OPERATIONS$P!{COPY} O WHERE O.ITEM = P.ID AND O.ENTITY = $P{ENTITY} AND O.DATE >= TO_TIMESTAMP($P!{FDCP}) AND O.DATE < TO_TIMESTAMP($P!{FDNP}) AND O.INCOME = FALSE) AS OUTCOMES,
			(SELECT SUM(CASE WHEN O.INCOME THEN O.AMMOUNT ELSE -O.AMMOUNT END) FROM OPERATIONS$P!{COPY} O WHERE O.ITEM = P.ID AND O.ENTITY = $P{ENTITY} AND O.DATE < TO_TIMESTAMP($P!{FDNP})) AS AT_END
		FROM ITEMS$P!{COPY} P JOIN MEASURE_UNITS M on P.MEASURE_UNIT = M.ID
		WHERE SUBSTRING(P.id::text, LENGTH(SUBSTRING(p.entity::text FROM '^[0-9]+')) + 1, 1) IN ('1', '2') 
		ORDER BY P.CODE) AS EXISTENCES
WHERE AT_BEGIN > 0
	OR INCOMES > 0
	OR OUTCOMES > 0
	OR AT_END > 0]]>
	</queryString>
