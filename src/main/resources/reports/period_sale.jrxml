	<queryString>
		<![CDATA[SELECT P.ID, P.DESCRIPTION, SUM(O.AMMOUNT) AS AMMOUNT,  SUM(O.VALUE) AS VALUE
FROM ITEMS$P!{COPY} P
JOIN OPERATIONS$P!{COPY} O ON P.ID = O.ITEM
WHERE O.INCOME = FALSE 
	 AND O.ENTITY = $P{ENTITY}
     AND O.OPENING = FALSE
	AND SUBSTRING(P.id::text, LENGTH(SUBSTRING(p.entity::text FROM '^[0-9]+')) + 1, 1) IN ('4', '5') 
	AND O.DATE >= TO_TIMESTAMP($P{FDCP})
	AND O.DATE < TO_TIMESTAMP($P{FDNP})
GROUP BY P.ID, P.DESCRIPTION
ORDER BY P.CODE]]>
	</queryString>
